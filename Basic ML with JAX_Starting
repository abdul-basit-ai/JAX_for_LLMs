      "source": [
        "import jax.numpy as jnp\n",
        "from jax import random\n",
        "\n",
        "def initialize_params(key, input_dim, output_dim):\n",
        "    w_key, b_key = random.split(key)\n",
        "    # Initialize weights with small random values\n",
        "    weights = random.normal(w_key, (input_dim, output_dim)) * 0.01\n",
        "    # Initialize biases to zeros\n",
        "    bias = jnp.zeros(output_dim)\n",
        "    return {'weights': weights, 'bias': bias}\n",
        "\n",
        "def sigmoid(x):\n",
        "    return 1.0 / (1.0 + jnp.exp(-x))\n",
        "\n",
        "def predict_proba(params, X):\n",
        "    # Linear combination of inputs and weights\n",
        "    logits = jnp.dot(X, params['weights']) + params['bias']\n",
        "    # Apply softmax for multi-class classification to get probabilities\n",
        "    probabilities = jax.nn.softmax(logits)\n",
        "    return probabilities\n",
        "\n",
        "# Initialize parameters for the Iris dataset\n",
        "# X has 4 features (input_dim), Y_one_hot has 3 classes (output_dim)\n",
        "key = random.key(0) # Master key\n",
        "key, init_key = random.split(key)\n",
        "input_dim = X.shape[1]\n",
        "output_dim = Y_one_hot.shape[1]\n",
        "\n",
        "params = initialize_params(init_key, input_dim, output_dim)\n",
        "\n",
        "print(f\"Initial weights shape: {params['weights'].shape}\")\n",
        "print(f\"Initial bias shape: {params['bias'].shape}\")\n",
        "\n",
        "# Test predict_proba with a sample\n",
        "sample_X = X[:1]\n",
        "probabilities = predict_proba(params, sample_X)\n",
        "print(f\"\\nSample input X shape: {sample_X.shape}\")\n",
        "print(f\"Predicted probabilities for sample: {probabilities}\")\n",
        "print(f\"Sum of probabilities for sample: {jnp.sum(probabilities)}\")"
      ],
      "execution_count": 124,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
