      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 0
        },
        "id": "1f805e42",
        "outputId": "7e6b37c6-f11f-41d3-b93e-2126f230f790"
      },
      "source": [
        "num_iterations_ransac = 2000\n",
        "threshold_ransac = 2.0\n",
        "\n",
        "best_params, best_inliers_indices = RANSAC(X_combined, Y_combined, num_iterations_ransac, threshold_ransac)\n",
        "\n",
        "# Ensure best_inliers_indices is not empty before proceeding\n",
        "if len(best_inliers_indices) == 0:\n",
        "    print(\"No inliers found by RANSAC. Cannot refit model.\")\n",
        "else:\n",
        "    X_inliers = X_combined[best_inliers_indices]\n",
        "    Y_inliers = Y_combined[best_inliers_indices]\n",
        "\n",
        "    print(f\"RANSAC initial best params (m, b): {best_params[0]:.4f}, {best_params[1]:.4f}\")\n",
        "    print(f\"Number of inliers found: {len(X_inliers)}\")\n",
        "\n",
        "    # Calculate means for OLS\n",
        "    mean_X_inliers = jnp.mean(X_inliers)\n",
        "    mean_Y_inliers = jnp.mean(Y_inliers)\n",
        "\n",
        "    # Compute refined slope (m_refined) using OLS\n",
        "    numerator = jnp.sum((X_inliers - mean_X_inliers) * (Y_inliers - mean_Y_inliers))\n",
        "    denominator = jnp.sum((X_inliers - mean_X_inliers)**2)\n",
        "\n",
        "    if denominator == 0:\n",
        "        m_refined = 0.0 # Handle case where all X_inliers are the same\n",
        "    else:\n",
        "        m_refined = numerator / denominator\n",
        "\n",
        "    # Compute refined intercept (b_refined) using OLS\n",
        "    b_refined = mean_Y_inliers - m_refined * mean_X_inliers\n",
        "\n",
        "    print(f\"Refined OLS Slope (m_refined): {m_refined:.4f}\")\n",
        "    print(f\"Refined OLS Intercept (b_refined): {b_refined:.4f}\")"
      ],
      "execution_count": 115,
      "outputs": [
        {
          "output_type": "stream",
