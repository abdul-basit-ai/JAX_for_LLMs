      "source": [
        "num_iterations_ransac = 2000\n",
        "threshold_ransac = 2.0\n",
        "\n",
        "best_params, best_inliers_indices = RANSAC(X_combined, Y_combined, num_iterations_ransac, threshold_ransac)\n",
        "\n",
        "# Ensure best_inliers_indices is not empty before proceeding\n",
        "if len(best_inliers_indices) == 0:\n",
        "    print(\"No inliers found by RANSAC. Cannot refit model.\")\n",
        "else:\n",
        "    X_inliers = X_combined[best_inliers_indices]\n",
        "    Y_inliers = Y_combined[best_inliers_indices]\n",
        "\n",
        "    print(f\"RANSAC initial best params (m, b): {best_params[0]:.4f}, {best_params[1]:.4f}\")\n",
        "    print(f\"Number of inliers found: {len(X_inliers)}\")"
      ],
