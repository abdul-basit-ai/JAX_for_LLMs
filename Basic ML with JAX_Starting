      "source": [
        "import jax.numpy as jnp\n",
        "\n",
        "def categorical_cross_entropy(params, X, Y_one_hot):\n",
        "    # Get predicted probabilities\n",
        "    y_pred_proba = predict_proba(params, X)\n",
        "\n",
        "    # Clip probabilities to avoid log(0) errors\n",
        "    y_pred_proba = jnp.clip(y_pred_proba, 1e-9, 1.0 - 1e-9)\n",
        "\n",
        "    # Calculate cross-entropy loss\n",
        "    # For one-hot encoded labels, this simplifies to -sum(Y_one_hot * log(y_pred_proba))\n",
        "    loss = -jnp.sum(Y_one_hot * jnp.log(y_pred_proba), axis=1)\n",
        "    return jnp.mean(loss)\n",
        "\n",
        "# Test the loss function with initial parameters\n",
        "initial_loss = categorical_cross_entropy(params, X, Y_one_hot)\n",
        "print(f\"Initial Categorical Cross-Entropy Loss: {initial_loss:.4f}\")"
      ],
      "execution_count": 125,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Initial Categorical Cross-Entropy Loss: 1.1177\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 0
        },
        "id": "5869a8ce",
        "outputId": "cbb092c0-bfc0-4c2b-f261-5cc428c99148"
      },
      "source": [
        "from jax import grad\n",
        "\n",
        "@jit # Just-in-time compile the training step for speed\n",
        "def train_step(params, X, Y_one_hot, learning_rate):\n",
        "    # Calculate gradients of the loss function with respect to params\n",
        "    grads = grad(categorical_cross_entropy)(params, X, Y_one_hot)\n",
        "\n",
        "    # Update weights and bias using gradient descent\n",
        "    new_weights = params['weights'] - learning_rate * grads['weights']\n",
        "    new_bias = params['bias'] - learning_rate * grads['bias']\n",
        "\n",
        "    return {'weights': new_weights, 'bias': new_bias}\n",
        "\n",
        "print(\"Defined the 'train_step' function for parameter updates.\")"
      ],
      "execution_count": 126,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Defined the 'train_step' function for parameter updates.\n"
          ]
        }
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 0
        },
        "id": "61ace96d",
        "outputId": "4f06de77-28e2-4f56-be37-c6bc6c2f808b"
      },
      "source": [
        "num_epochs = 5000\n",
        "learning_rate = 0.01\n",
        "\n",
        "# Initialize a list to store losses for plotting\n",
        "loss_history = []\n",
        "\n",
        "for epoch in range(num_epochs):\n",
        "    params = train_step(params, X, Y_one_hot, learning_rate)\n",
        "    current_loss = categorical_cross_entropy(params, X, Y_one_hot)\n",
        "    loss_history.append(current_loss)\n",
        "\n",
        "    if epoch % 1000 == 0:\n",
        "        print(f\"Epoch {epoch}, Loss: {current_loss:.4f}\")\n",
        "\n",
        "final_loss = categorical_cross_entropy(params, X, Y_one_hot)\n",
        "print(f\"\\nTraining complete. Final Loss: {final_loss:.4f}\")\n",
        "print(f\"Final weights: {params['weights']}\")\n",
        "print(f\"Final bias: {params['bias']}\")"
