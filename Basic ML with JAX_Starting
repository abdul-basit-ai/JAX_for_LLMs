      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Create a meshgrid to plot the decision boundary\n",
        "x_min, x_max = X_2d[:, 0].min() - 1, X_2d[:, 0].max() + 1\n",
        "y_min, y_max = X_2d[:, 1].min() - 1, X_2d[:, 1].max() + 1\n",
        "h = 0.02 # Step size in the mesh\n",
        "xx, yy = jnp.meshgrid(jnp.arange(x_min, x_max, h), jnp.arange(y_min, y_max, h))\n",
        "\n",
        "# Predict probabilities for each point in the meshgrid\n",
        "Z_proba = predict_proba(params_2d, jnp.c_[xx.ravel(), yy.ravel()])\n",
        "\n",
        "# Get the predicted class (the one with the highest probability)\n",
        "Z = jnp.argmax(Z_proba, axis=1)\n",
        "Z = Z.reshape(xx.shape)\n",
        "\n",
        "# Plot the decision boundary\n",
        "plt.figure(figsize=(10, 8))\n",
        "plt.contourf(xx, yy, Z, cmap=plt.cm.RdYlBu, alpha=0.8)\n",
        "\n",
        "# Plot the training points\n",
        "colors = ['red', 'green', 'blue'] # Colors for the three classes\n",
        "for i, color in enumerate(colors):\n",
        "    idx = jnp.where(Y == i)\n",
        "    plt.scatter(X_2d[idx, 0], X_2d[idx, 1], c=color, label=iris.target_names[i], edgecolor='k', s=20)\n",
        "\n",
        "plt.xlabel('Sepal length')\n",
        "plt.ylabel('Sepal width')\n",
        "plt.title('Logistic Regression Decision Boundary (Iris Dataset - 2 Features)')\n",
        "plt.legend()\n",
        "plt.show()\n"
      ],
