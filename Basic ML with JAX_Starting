      "source": [
        "<p>6. Fit linear model with RANSAC algorithm</p>"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "1be8d955"
      },
      "source": [
        "def RANSAC(X, Y, num_iterations, threshold):\n",
        "    best_inliers = jnp.array([])\n",
        "    best_params = None\n",
        "    best_inliers_count = 0\n",
        "\n",
        "    for i in range(num_iterations):\n",
        "        # Step 3: Randomly select two distinct data points\n",
        "        # Ensure X is 2D for consistent indexing\n",
        "        if X.ndim == 1:\n",
        "            X = X.reshape(-1, 1)\n",
        "\n",
        "        key_sample, _ = random.split(random.PRNGKey(i))\n",
        "        idx = random.choice(key_sample, X.shape[0], shape=(2,), replace=False)\n",
        "\n",
        "        X_sample = X[idx]\n",
        "        Y_sample = Y[idx]\n",
        "\n",
        "        x1, y1 = X_sample[0, 0], Y_sample[0, 0]\n",
        "        x2, y2 = X_sample[1, 0], Y_sample[1, 0]\n",
        "\n",
        "        # Step 4: Calculate the slope m_candidate and intercept b_candidate\n",
        "        if jnp.isclose(x2 - x1, 0.0):\n",
        "            continue\n",
        "\n",
        "        m_candidate = (y2 - y1) / (x2 - x1)\n",
        "        b_candidate = y1 - m_candidate * x1\n",
        "\n",
        "        # Step 5: Predict the Y values for all data points\n",
        "        Y_pred_candidate = m_candidate * X + b_candidate\n",
        "\n",
        "        # Step 6: Calculate the absolute difference (vertical distance)\n",
        "        distances = jnp.abs(Y_pred_candidate - Y)\n",
        "\n",
        "        # Step 7: Identify inliers\n",
        "        current_inliers_mask = distances <= threshold\n",
        "        current_inliers_count = jnp.sum(current_inliers_mask)\n",
        "        current_inliers_indices = jnp.where(current_inliers_mask.flatten())[0]\n",
        "\n",
        "        # Step 8: Update best model if current model is better\n",
        "        if current_inliers_count > best_inliers_count:\n",
        "            best_inliers_count = current_inliers_count\n",
        "            best_params = (m_candidate, b_candidate)\n",
        "            best_inliers = current_inliers_indices\n",
        "\n",
        "    return best_params, best_inliers"
      ],
      "execution_count": 113,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
