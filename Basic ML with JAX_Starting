        "### Adam Optimizer"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "def mse_loss(params, X, Y):\n",
        "    m, b = params\n",
        "    predictions = m * X + b\n",
        "    return jnp.mean((predictions - Y) ** 2)"
      ],
      "metadata": {
        "id": "ltvHLtyOnFO_"
      },
      "execution_count": 107,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 599
        },
        "id": "026d55fd",
        "outputId": "725e515d-2008-467c-d71d-61595a956276"
      },
      "source": [
        "def adam_optimizer(X, Y, initial_params, learning_rate, beta1, beta2, epsilon, iterations):\n",
        "    params = initial_params\n",
        "    m_t = (jnp.zeros_like(params[0]), jnp.zeros_like(params[1])) # Initialize first moment vector\n",
        "    v_t = (jnp.zeros_like(params[0]), jnp.zeros_like(params[1])) # Initialize second moment vector\n",
        "\n",
        "    params_history = [params]\n",
        "\n",
        "    gradient_fn = jax.grad(mse_loss)\n",
        "\n",
        "    for t in range(1, iterations + 1):\n",
        "        grads = gradient_fn(params, X, Y)\n",
        "        # First Moment\n",
        "        m_t = (beta1 * m_t[0] + (1 - beta1) * grads[0],\n",
        "               beta1 * m_t[1] + (1 - beta1) * grads[1])\n",
        "\n",
        "        # Second Moment\n",
        "        v_t = (beta2 * v_t[0] + (1 - beta2) * (grads[0]**2),\n",
        "               beta2 * v_t[1] + (1 - beta2) * (grads[1]**2))\n",
        "\n",
        "        # Compute bias-corrected first moment estimate\n",
        "        m_hat_t = (m_t[0] / (1 - beta1**t),\n",
        "                   m_t[1] / (1 - beta1**t))\n",
        "\n",
        "        # Compute bias-corrected second moment estimate\n",
        "        v_hat_t = (v_t[0] / (1 - beta2**t),\n",
        "                   v_t[1] / (1 - beta2**t))\n",
        "\n",
        "        new_m = params[0] - learning_rate * m_hat_t[0] / (jnp.sqrt(v_hat_t[0]) + epsilon)\n",
        "        new_b = params[1] - learning_rate * m_hat_t[1] / (jnp.sqrt(v_hat_t[1]) + epsilon)\n",
        "        params = (new_m, new_b)\n",
        "\n",
        "        params_history.append(params)\n",
        "\n",
        "    return params, params_history\n",
        "\n",
        "key_params_adam, key = random.split(key)\n",
        "initial_m_adam = random.normal(key_params_adam, ()) * 0.1\n",
        "initial_b_adam = random.normal(key_params_adam, ()) * 0.1\n",
        "initial_params_adam = (initial_m_adam, initial_b_adam)\n",
        "\n",
        "# Adam hyperparameters\n",
        "learning_rate_adam = 0.01\n",
        "beta1 = 0.9\n",
        "beta2 = 0.999\n",
        "epsilon = 1e-8\n",
        "num_iterations_adam = 2000\n",
        "\n",
        "# Run the Adam optimizer\n",
        "final_params_adam, history_params_adam = adam_optimizer(\n",
        "    X, Y, initial_params_adam, learning_rate_adam, beta1, beta2, epsilon, num_iterations_adam\n",
        ")\n",
        "\n",
        "m_adam, b_adam = final_params_adam\n",
        "\n",
        "print(f\"Adam Optimizer (Combined Data) - Estimated Slope (m): {m_adam:.4f}\")\n",
        "print(f\"Adam Optimizer (Combined Data) - Estimated Intercept (b): {b_adam:.4f}\")\n",
        "\n",
        "# Predict Y values using the Adam coefficients\n",
        "Y_pred_adam = predict(final_params_adam, X)\n",
        "\n",
        "# Plotting the results\n",
        "plt.figure(figsize=(10, 6))\n",
        "plt.scatter(X, Y, s=10, alpha=0.6, label='Original Data')\n",
        "# plt.scatter(X_outliers, Y_outliers, s=30, color='red', marker='x', label='Outliers')\n",
        "\n",
        "# Sort X_combined for proper line plotting\n",
        "sorted_indices = jnp.argsort(X.flatten())\n",
        "plt.plot(\n",
        "    X[sorted_indices],\n",
        "    Y_pred_adam[sorted_indices],\n",
        "    color='green',\n",
        "    label=f'Adam Fitted Line (y = {m_adam:.2f}x + {b_adam:.2f})'\n",
        ")\n",
        "plt.xlabel('X')\n",
        "plt.ylabel('Y')\n",
        "plt.title('Linear Regression with Adam Optimizer on Combined Data (including Outliers)')\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()"
      ],
      "execution_count": 108,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "Adam Optimizer (Combined Data) - Estimated Slope (m): 2.0525\n",
            "Adam Optimizer (Combined Data) - Estimated Intercept (b): 2.9016\n"
          ]
